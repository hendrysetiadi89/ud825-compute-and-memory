                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".MainActivity" />
        </activity>

        <activity
                android:name=".renderoptimized2.DroidCardsActivity"
                android:label="@string/title_activity_gpu_overdraw_2"
                android:parentActivityName=".MainActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".MainActivity" />
        </activity>
    </application>

</manifest>